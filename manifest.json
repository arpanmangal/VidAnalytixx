{
  "manifest_version": 2,
  "name": "VidAnalytixx",
  "version": "1.0",

  "description": "An extension to get detailed analytics of user attentivenss and emotions during viewing a video.",
  "homepage_url": "https://github.com/ArpanMangal/VidAnalytixx",

  "icons": {
    "48": "logo.png"
  },

  "permissions": [
    "*://southeastasia.api.cognitive.microsoft.com/*",
    "*://*.google.co.in/*",
    "activeTab",
    "webRequest"
  ],

  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "VidAnalytixx",
    "default_popup": "popup.html"
  },

  "web_accessible_resources": [
    "extra/child.html"
  ]



}
